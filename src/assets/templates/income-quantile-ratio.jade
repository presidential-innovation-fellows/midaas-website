.widget-header
  h2.chart-title
  p United States of America
.chart
#loading-icon.loading-icon.active
  img(src="/assets/img/loading-ring.svg")
#chart-error.chart-error
  .error-icon
    i.fa.fa-exclamation-triangle
  .error-message
    p#error-message
      | Something went wrong... Try again in a few seconds.
select#compareQuantile
  option(value=10) 10%
  option(value=20) 20%
  option(value=30) 30%
  option(value=40) 40%
  option(value=50) 50%
  option(value=60) 60%
  option(value=70) 70%
  option(value=80) 80%
  option(value=90) 90%
  option(value=95) 95%
  option(value=99) 99%
